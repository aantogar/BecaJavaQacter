package ejerciciosBanco;

import java.time.LocalDate;
import java.util.Calendar;

import javax.xml.crypto.Data;

public class testBanco {
	public static void main (String[]args) throws Exception {
		
		LocalDate date=LocalDate.now();
		Debito deb=new Debito("33456","Andrea",date);
		deb.ingresar(500);
		System.out.println(deb.getSaldo());
		System.out.println(deb.getMovimientos());
		Cuenta c1=new Cuenta();
		Debito dbb=new Debito("33456","Andrea",date);
		c1.ingresar(700);
		c1.ingresar(200);
		System.out.println(c1.getMovimientos());
		dbb.getCuentasoc().ingresar(600);
		c1.retirar(200);
		dbb.ingresar(2000);
		dbb.getCuentasoc().ingresar(1000);
		//System.out.println(dbb.getCuentasoc().getSaldo());
		Credito cred=new Credito("33456","Andrea",date);
		cred.getCuentasoc().ingresar(500);
		System.out.println(cred.getCuentasoc().getMovimientos());
		
		
	}

}
