package productosEjercicio;

public class Frescos extends Productos{
	private String pais;
	private String fechaEnv;
	
	public Frescos(String pais,String fecha) {
		super("2020-10-01",0);
		this.pais=pais;
		this.fechaEnv=fecha;
	}

	@Override
	public String toString() {
		return "Productos frescos de procedencia de : " +getPais() + ", con fecha de envase" + getFechaEnv() + ", su fecha de caducidad: " + getFechaCaduc()
				+ "y n√∫mero de lote: " + getNumLote() + ".";
	}

	public String getPais() {
		return pais;
	}

	public void setPais(String pais) {
		this.pais = pais;
	}

	public String getFechaEnv() {
		return fechaEnv;
	}

	public void setFechaEnv(String fechaEnv) {
		this.fechaEnv = fechaEnv;
	}

}
