package excepcionesEjerciciones;


public class ManejoExcepciones {

	
	
	public static void main (String[]args)throws ArithmeticException {
		int a=0;
		int b=300;
		int c;
		try {
			c=a/b;
			System.out.println("El resultado es: "+c);
			
		
		}finally{
			System.out.println("Programa finalizado");
			
		}
		
	}


}
