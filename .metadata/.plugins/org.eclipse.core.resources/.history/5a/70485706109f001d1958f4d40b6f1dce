package bancoAppPracticar;

public class Direccion {
	private String calle;
	private String ciudad;
	private String pais;
	private String codPos;
	
	public Direccion() {
		
	}
	public Direccion(String calle,String ciudad,String pais,String codPos) {
		this.calle=calle;
		this.ciudad=ciudad;
		this.pais=pais;
		this.codPos=codPos;
	}

}
